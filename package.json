{
  "name": "@jaebradley/react-pluralize",
  "version": "0.0.0-development",
  "description": "React component for pluralized strings",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "pluralize"
  ],
  "homepage": "https://github.com/jaebradley/react-pluralize#readme",
  "bugs": {
    "url": "https://github.com/jaebradley/react-pluralize/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/react-pluralize.git"
  },
  "license": "MIT",
  "author": {
    "name": "jae.b.bradley@gmail.com"
  },
  "files": [
    "build"
  ],
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "scripts": {
    "build": "rollup -c",
    "build:prod": "BABEL_ENV=production npm run build",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "deploy": "npm run travis-deploy-once 'npm run semantic-release'",
    "gc": "commit",
    "lint": "eslint --ext .js,.jsx .",
    "prepare": "npm run build:prod",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook-ci": "storybook-to-ghpages --ci",
    "test": "BABEL_ENV=test jest",
    "travis-deploy-once": "travis-deploy-once"
  },
  "browser": "build/index.js",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/setupTest.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(.stories)\\.(jsx)$",
      "<rootDir>/src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./src/setupTest.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/setupTest.js"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "pluralize": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@commitlint/prompt-cli": "^8.3.5",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-centered": "^5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/channels": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "autoprefixer": "^9.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "26.0.1",
    "babel-loader": "^8.1.0",
    "codecov": "^3.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "node-sass": "^7.0.0",
    "postcss": "^7.0.32",
    "rollup": "^2.16.1",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-terser": "^6.1.0",
    "sass-loader": "^8.0.2",
    "semantic-release": "^17.0.8",
    "source-map-loader": "^1.0.0",
    "travis-deploy-once": "^5.0.11",
    "webpack": "^4.43.0",
    "webpack-build-notifier": "^2.0.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "_id": "react-pluralize@0.0.0-development",
  "readme": "https://github.com/jaebradley/react-pluralize#readme"
}
